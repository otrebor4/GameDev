import Polygon
import game.util.Vector2.Vector2 as Vector2
class Rectangle(Polygon.Polygon):
    def __init__(self, (x,y) = (0,0), (w,h) = (0,0) ):
        points = [Vector2(0,0),
                  Vector2(w,0),
                  Vector2(w,h),
                  Vector2(0,h)]
        Polygon.Polygon.__init__(self, (x, y), points)