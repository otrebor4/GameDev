import game.util.vector2 as vector2
import phyObj


class CircleCollider(phyObj.Collider):
    def __init__(self, gameObject, position=vector2.Vector2(0, 0), radius=0):
        phyObj.Collider.__init__(self, gameObject)
        self.radius = radius
        self.position = position
