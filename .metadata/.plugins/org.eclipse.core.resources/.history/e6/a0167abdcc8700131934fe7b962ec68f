import phys.physEng as physEng
import sprite,gameObject

class World:
    
    def __init__(self):
        self.phyEng = physEng.PhysEng()
        
    #wall are static object don't move
    def addWall(self, imgOrCollor, x,y, w, h):
        if img == None:
            wall = gameObject.RectObject( )
        else 
            wall = sprite.Sprite( img, x, y, w, h)    
        self.phyEng.add( wall )
        
    