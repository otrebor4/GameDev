import game
class Sprite:
    
    # sprite constructor
    def __init__(self, img, x,y, w, h):
        self.img = img
        self.x = x
        self.y = y
        self.w = w
        self.h = h
        pass
    
    # draw sprite
    def draw(self,screen):
        img = game.pygame.image.load()
        img.get_rect()
        if self.img != None:
            screen.blit(self.img, )
    
        pass
    
