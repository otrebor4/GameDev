class PhysEng:
    
    def __init__(self):
        self.objects = []
        
    def objCount(self):
        return len(self.objects)
    
    def add(self, obj):
        self.objects.append(obj)
    
    def remove(self, obj):
        self.objects.remove(obj)
        
    