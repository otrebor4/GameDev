class Shape:
    def __init__(self):
        self.aabb = [0,0,0,0]
    
    
    def Top(self):
        return self.aabb[0]
    def Righ(self):
        return self.aabb[1]
    def Bottom(self):
        return self.aabb[2]
    def Left(self):
        return self.aabb[3]
    
