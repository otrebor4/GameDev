import game.util.Vector2 as Vector2
class Shape:
    def __init__(self, (x,y)):
        self.positino = Vector2.Vector2(x,y)
        self.aabb = [0,0,0,0]
    
    
    def Top(self):
        return self.aabb[0]
    def Righ(self):
        return self.aabb[1]
    def Bottom(self):
        return self.aabb[2]
    def Left(self):
        return self.aabb[3]
    
    
    
    